  <section-def-macros>
    <def-macro n="when_are_we1" npar="1">
	<choose>
	  <when>
	    <test>
                <equal caseless="yes">
                  <clip pos="1" part="temps"/>
                  <lit-tag v="pii"/>
                </equal>
            </test>
	    <let>
	      <var n="tense"/>
              <lit-tag v="past"/>
	    </let>
	    <let>
              <clip pos="1" part="temps"/>
              <lit-tag v="past"/>
	    </let>
	  </when>
	  <when>
	    <test>
              <and>
                <equal caseless="yes">
                  <clip pos="1" part="lem"/>
                  <lit v="verbcj"/>
                </equal>
                <not>
                  <equal caseless="yes">
                    <clip pos="1" part="temps"/>
                    <lit-tag v="pii"/>
                  </equal>
                </not>
              </and>
            </test>
	    <let>
	      <var n="tense"/>
	      <clip pos="1" part="temps"/>
	    </let>
	  </when>
	  <otherwise>
            <let>
              <var n="tense"/>
	      <lit-tag v="pres"/>
	    </let>
	  </otherwise>
	</choose>
    </def-macro>

