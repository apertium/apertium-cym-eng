    <rule comment="REGLA: Vpart">
      <pattern>
        <pattern-item n="Vpart"/>
      </pattern>
      <action>
        <choose>
	  <when>
            <test>
	      <equal>
	        <clip pos="1" part="tags"/>
	        <lit-tag v="Vpart.neg"/>
	      </equal>
	    </test>
            <out>
              <chunk>
                <lit v="adv"/>
                <lit-tag v="Adv"/>
                <lit v="{^not"/>
                <lit-tag v="adv"/>
                <lit v="$}"/>
              </chunk>
            </out>
            <let>
              <var n="posa_neg"/>
              <lit v="1"/>
            </let>
	  </when>
          <otherwise>
	    <out>
              <b/>  
	    </out>
	  </otherwise>
	</choose>
      </action>
    </rule>


